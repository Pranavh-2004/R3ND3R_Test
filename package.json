{
  "name": "r3nd3r",
  "displayName": "R3ND3R",
  "description": "Render and test React.js components on the fly",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:r3nd3r.openVisualizer"
  ],
  "contributes": {
    "commands": [
      {
        "command": "r3nd3r.openVisualizer",
        "title": "Open Code Visualizer"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "compile": "webpack --config webpack.config.js",
    "watch": "webpack --watch --config webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "@babel/preset-react": "^7.18.6",
    "@babel/standalone": "^7.19.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "ts-loader": "^9.5.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "babel-loader": "^9.1.2",
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.23.3"
  }
}
